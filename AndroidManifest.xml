		android:icon="@drawable/icon"
		android:label="@string/app_name"
		android:theme="@android:style/Theme.NoTitleBar">
		<activity
			android:name=".IsraelPack"
			android:label="@string/app_name">
			<intent-filter>
				<action
					android:name="android.intent.action.MAIN" />
				<category
					android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<meta-data
			android:name="com.a0soft.gphone.aTrackDog.webURL"
			android:value="http://code.google.com/p/israelpack/" />
		<meta-data
			android:name="com.a0soft.gphone.aTrackDog.dlURL"
			android:value="http://israelpack.googlecode.com/files/IsraelPack_latest.apk" />
		<meta-data
			android:name="com.a0soft.gphone.aTrackDog.noMarket"
			android:value="true" />
	</application>
	<uses-sdk
		android:minSdkVersion="3" />
	<uses-permission
		android:name="android.permission.INTERNET">
	</uses-permission>
	<uses-permission
		android:name="android.permission.WRITE_EXTERNAL_STORAGE">
	</uses-permission>
